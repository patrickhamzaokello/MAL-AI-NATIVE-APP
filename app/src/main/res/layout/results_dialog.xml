<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dialog_background"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/corner_circle"
    android:backgroundTint="@color/white"
    android:orientation="vertical">


    <ImageButton
        android:id="@+id/closebtn"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_margin="20dp"
        android:layout_marginStart="15dp"
        android:background="@drawable/rounded_card"
        android:backgroundTint="#14000000"
        android:contentDescription="@string/app_name"
        android:padding="4dp"
        android:src="@drawable/ic_close"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:tint="@color/primary_color" />


    <TextView
        android:id="@+id/title_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:ellipsize="end"
        android:fontFamily="@font/poppinsbold"
        android:gravity="left"
        android:text="Analytics Results"
        android:textColor="@color/primary_color"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/closebtn" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/plasmodium_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/smll_rounded_btn"
        android:backgroundTint="@color/background_green"
        android:paddingVertical="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintTop_toBottomOf="@+id/result_barrier">


    <TextView
        android:id="@+id/plasmodium_species_lable"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="10dp"
        android:ellipsize="end"
        android:fontFamily="@font/poppinsregular"
        android:gravity="left"
        android:maxLines="2"
        android:text="Plasmodium Species"
        android:textColor="@color/primary_color"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/plasmodium_species_value"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:ellipsize="end"
        android:fontFamily="@font/poppinsbold"
        android:gravity="left"
        android:maxLines="2"
        android:text="40%"
        android:textColor="@color/primary_color"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/plasmodium_species_lable" />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/gametocytes_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/smll_rounded_btn"
        android:backgroundTint="@color/background_green"
        android:paddingVertical="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/plasmodium_layout">

        <TextView
            android:id="@+id/gametocytes_lable"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            android:layout_marginTop="5dp"
            android:ellipsize="end"
            android:fontFamily="@font/poppinsregular"
            android:gravity="left"
            android:maxLines="2"
            android:text="Gametocytes"
            android:textColor="@color/primary_color"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/gametocytes_value"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            android:ellipsize="end"
            android:fontFamily="@font/poppinsbold"
            android:gravity="left"
            android:maxLines="2"
            android:text="50%"
            android:textColor="@color/primary_color"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/gametocytes_lable" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <View
        android:id="@+id/result_barrier"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginHorizontal="20dp"
        android:background="@color/primary_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/title_text" />

    <Button
        android:id="@+id/playTrack"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_btn_bg"
        android:fontFamily="@font/poppinsbold"
        android:text="Close"
        android:textAllCaps="false"
        android:textColor="#FAFAFA"
        app:backgroundTint="@color/primary_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gametocytes_layout" />


</androidx.constraintlayout.widget.ConstraintLayout>