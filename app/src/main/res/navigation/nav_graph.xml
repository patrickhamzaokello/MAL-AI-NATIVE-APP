<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:label="@string/title_home"
        android:name="com.pkasemer.malai.Fragments.HomeFragment"
        tools:layout="@layout/fragment_home" >
    </fragment>



    <fragment
        android:id="@+id/navigation_report"
        android:name="com.pkasemer.malai.Fragments.GalleryFragment"
        tools:layout="@layout/fragment_report"
        android:label="@string/title_search"
        />



    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.pkasemer.malai.Fragments.SettingsFragment"
        tools:layout="@layout/fragment_settings"
        android:label="@string/title_library" />


</navigation>